<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <!--
      This is a default configuration, it is re-configured by the CacheConfiguration Spring Bean, using the
      properties from the resources/config/*.yml files.
    -->

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="com.pikiranrakyat.prevent.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.pikiranrakyat.prevent.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.pikiranrakyat.prevent.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.pikiranrakyat.prevent.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.pikiranrakyat.prevent.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>


    <cache name="com.pikiranrakyat.prevent.social.SocialUserConnection"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.pikiranrakyat.prevent.domain.FileManager"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.pikiranrakyat.prevent.domain.ContactPerson"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.pikiranrakyat.prevent.domain.master.Locations"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.pikiranrakyat.prevent.domain.master.EventType"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.pikiranrakyat.prevent.domain.Organizer"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.pikiranrakyat.prevent.domain.master.Merchandise"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.pikiranrakyat.prevent.domain.AdsCategory"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.pikiranrakyat.prevent.domain.master.Ads"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.pikiranrakyat.prevent.domain.master.Circulation"
        timeToLiveSeconds="3600">
    </cache>

    <!-- jhipster-needle-ehcache-add-entry -->
</ehcache>
